    <hudson.tasks.Shell>
      <command>#!/bin/bash
#set -xv
TAG=LATEST_SUCCESSFULL
cd $WORKSPACE/
DOESTAGEXISTCMD=&quot;git tag -l | grep $TAG&quot;
eval $DOESTAGEXISTCMD
if [ $? -eq 0 ]; then
	git tag -d $TAG
	git push origin :$TAG
fi
git tag -a $TAG -m &quot;Jenkins develop&quot;
git push origin $TAG --force

#git fetch --prune origin +refs/tags/*:refs/tags/
git fetch --tags

#git show $TAG

GIT_LATEST_SUCCESSFUL_COMMIT=$(git rev-parse --short $TAG)
echo &quot;GIT_LATEST_SUCCESSFUL_COMMIT= ${GIT_LATEST_SUCCESSFUL_COMMIT}&quot;

#make version file
cd ${WORKSPACE}/
FILENAME=$JOB_NAME&quot;_VERSION.TXT&quot;
echo &quot;$JOB_NAME: BUILD: $BUILD_NUMBER SHA1:${GIT_LATEST_SUCCESSFUL_COMMIT}&quot; &gt; ${WORKSPACE}/$FILENAME

#node --version
node --version || true
#npm --version
npm --version || true
./clean.sh
bower --version || true
#./bower list

rm -Rf .node_cache/ .node_tmp/  .tmp/ .bower/ node node_modules/ package-lock.json yarn.lock components/*/package-lock.json components/*/yarn.lock
</command>
    </hudson.tasks.Shell>
